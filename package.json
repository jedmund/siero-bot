{
  "name": "siero-bot",
  "version": "2.0.0",
  "description": "A rewrite of siero-bot using Sapphire",
  "main": "build/dist/index.js",
  "author": "Justin Edmund <justin@jedmund.com>",
  "license": "MIT",
  "dependencies": {
    "@sapphire/decorators": "^6.0.2",
    "@sapphire/framework": "^4.8.5",
    "@sapphire/plugin-subcommands": "^7.0.1",
    "@types/node": "^22.13.13",
    "chance": "^1.1.12",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "kysely": "^0.27.2",
    "pg": "^8.11.3",
    "pluralize": "^8.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/chance": "^1.1.6",
    "@types/pg": "^8.11.11",
    "@types/pluralize": "^0.0.33",
    "concurrently": "^8.2.2",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.28.0"
  },
  "scripts": {
    "build-ts": "rm -rf build/dist && tsc",
    "postinstall": "npm run build-ts",
